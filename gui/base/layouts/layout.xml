<layouts>
    <layout id="TOTPLayout">
        <drawable id="innerCircle" class="CircleProgress">
            <param name="start">90.1</param>
            <param name="end">90</param>
            <param name="offset">5</param>
            <param name="thickness">4</param>
            <param name="color">Graphics.COLOR_RED</param>
        </drawable>
    </layout>
    <layout id="HOTPLayout">
        <bitmap id="menu" personality="system_icon_dark__hint_action_menu system_loc__hint_action_menu" />
    </layout>
    <layout id="AuthentificatorLayout">
        <label id="code" text="" font="Graphics.FONT_NUMBER_MEDIUM" x="center" y="center" justification="Graphics.TEXT_JUSTIFY_CENTER" />
        <label id="name" text="" font="Graphics.FONT_MEDIUM" x="center" y="70%" justification="Graphics.TEXT_JUSTIFY_CENTER" />
    </layout>
    <layout id="NoAccountsLayout">
        <text-area id="mainLabel" text="@Strings.NoAccounts" personality="prompt_color_dark__body prompt_size__body_no_title prompt_loc__body_no_title prompt_font__body_no_title" />
    </layout>
</layouts>